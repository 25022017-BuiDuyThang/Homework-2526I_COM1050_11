    "#1\n",
    "def reverse(n):\n",
    "    res = 0\n",
    "    while n > 0:\n",
    "        res = res * 10 + n%10\n",
    "        n //= 10\n",
    "    return res\n",
    "num = 0\n",
    "with open(\"input.txt\",\"r\") as file: \n",
    "    num = file.read() \n",
    "num = int(num)\n",
    "print(reverse(num))"
